<!--Modal reultatModal-->
<div class="modal fade bd-example-modal-lg" id="reultatModal" tabindex="-1" role="dialog"
     aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        {{ form_start(form) }}
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">RÃ©sultat</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="form-group">
                    <div class="custom-file">
                        {{ form_widget(form.resultatPdf) }}
                    </div>
                </div>

            </div>
            <div class="modal-footer">
                <div class="row">
                    <div class="col-1">
                        <button type="submit"
                                class="btn btn-success float-right">{{ button_label|default('Envoyer') }}</button>
                    </div>
                    <div class="col-6">
                        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    </div>

                </div>
            </div>
        </div>
        {{ form_end(form) }}
    </div>
</div>
<!--/Modal-->
