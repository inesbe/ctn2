<!DOCTYPE html>

<title>Container</title>

{% block body %}
    <h1>Container</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ container.id }}</td>
            </tr>
            <tr>
                <th>Num_plomb</th>
                <td>{{ container.numPlomb }}</td>
            </tr>
            <tr>
                <th>Date_chargement</th>
                <td>{{ container.dateChargement ? container.dateChargement|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Date_enlevement</th>
                <td>{{ container.dateEnlevement ? container.dateEnlevement|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Poids_tare</th>
                <td>{{ container.poidsTare }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ container.type }}</td>
            </tr>
            <tr>
                <th>Temperature</th>
                <td>{{ container.temperature }}</td>
            </tr>
            <tr>
                <th>Prorietaire</th>
                <td>{{ container.prorietaire }}</td>
            </tr>
            <tr>
                <th>Reference</th>
                <td>{{ container.reference }}</td>
            </tr>
            <tr>
                <th>Poids_brute</th>
                <td>{{ container.poidsBrute }}</td>
            </tr>
            <tr>
                <th>Unite_payante</th>
                <td>{{ container.unitePayante }}</td>
            </tr>
            <tr>
                <th>Nb_colis</th>
                <td>{{ container.nbColis }}</td>
            </tr>
            <tr>
                <th>Id_utilisateur</th>
                <td>{{ container.idUtilisateur }}</td>
            </tr>
            <tr>
                <th>Id_reservation</th>
                <td>{{ container.idReservation }}</td>
            </tr>
            <tr>
                <th>Bl</th>
                <td>{{ container.bl }}</td>
            </tr>
            <tr>
                <th>Etat</th>
                <td>{{ container.etat }}</td>
            </tr>
            <tr>
                <th>Bl_etat</th>
                <td>{{ container.blEtat }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('container_index') }}">back to list</a>

    <a href="{{ path('container_edit', {'id': container.id}) }}">edit</a>

    {{ include('container/_delete_form.html.twig') }}
{% endblock %}
